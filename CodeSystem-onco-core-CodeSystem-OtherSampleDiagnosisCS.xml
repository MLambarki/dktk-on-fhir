  <concept>
    <code value="healthy"/>
    <display value="Healthy"/>
    <definition value="Sample contains healthy material"/>
  </concept>
  <concept>
    <code value="unknown"/>
    <display value="Unknown"/>
  </concept>
  <concept>
    <code value="not-applicable"/>
    <display value="Not applicable"/>
  </concept>
  <concept>
    <code value="unspecified-tumor"/>
    <display value="Unspecified tumor"/>
    <definition value="Sample contains tumor material, a specific diagnosis is not known"/>
  </concept>
</CodeSystem>
